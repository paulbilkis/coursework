#include <stdio.h>

int iscorrect(int *res, int el){
  int i, sum=0;
  for (i=0; i<5; i++)
    sum+=res[i];
  if ((sum + el) <= 7) return 1;
  else return 0;
}

int recursive (int *p, int *res, int j, int cur){
  if (j >= 5) return 1;
  int i;
  for (i = j; i<5; i++){
    if (iscorrect (res, p[i])){
      res[cur+1] = p[i];
      if (recursive (p, res, i+1, cur+1)) return 1;
      res[cur+1] = 0; //backtrace
    }
  }

  return 0;
}

int main (void){
  int p[5] = {1,2,3,4,5};
  int res[5]={0,0,0,0,0};
}
